{
  "name": "react-tailwind",
  "version": "4.0.0",
  "description": "Create React App with Tailwind CSS",
  "license": "MIT",
  "author": {
    "name": "Amit Agarwal",
    "email": "amit@labnol.org",
    "url": "https://digitalinspiration.com/"
  },
  "private": false,
  "scripts": {
    "dev:tailwind": "tailwindcss -i src/styles/tailwind.css -o src/styles/index.css -w",
    "dev:react": "react-scripts start",
    "start": "npm-run-all --parallel dev:*",
    "tailwind:build": "NODE_ENV=production tailwindcss -i src/styles/tailwind.css -o src/styles/index.css --minify",
    "prebuild": " npm run tailwind:build",
    "build": "react-scripts build",
    "inline": "npm run build && gulp"
  },
  "keywords": [
    "react",
    "tailwindcss",
    "tailwindui",
    "create-react-app"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@emailjs/browser": "^3.6.2",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@headlessui/react": "^1.6.2",
    "@heroicons/react": "^1.0.6",
    "@mui/material": "^5.5.3",
    "@mui/styled-engine-sc": "^5.5.2",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "assert": "^2.0.0",
    "axios": "^0.27.2",
    "buffer": "^5.7.1",
    "crypto": "^1.0.1",
    "crypto-browserify": "^3.12.0",
    "i18next": "^21.8.7",
    "i18next-browser-languagedetector": "^6.1.4",
    "metamask-react": "^2.3.0",
    "os": "^0.1.2",
    "os-browserify": "^0.3.0",
    "react": "^18.0.0",
    "react-back-to-top": "^1.0.3",
    "react-country-flag": "^3.0.2",
    "react-dom": "^18.0.0",
    "react-floaterjs": "^1.0.6",
    "react-i18next": "^11.16.10",
    "react-notifications": "^1.7.3",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "4.0.3",
    "stream": "^0.0.2",
    "styled-components": "^5.3.5",
    "tailwindcss": "^3.0.24",
    "web-vitals": "^2.1.4",
    "web3": "^1.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.4",
    "gulp": "^4.0.2",
    "gulp-inline-source": "^4.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12"
  },
  "browser": {
    "crypto": false
  }
}
